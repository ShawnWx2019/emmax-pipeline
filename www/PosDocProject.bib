  keywords = {/unread,Animals,Bacteria,Bacteroides,Gastrointestinal Microbiome,{Genes, Bacterial},Genomics,Host Microbial Interactions,Humans,Male,Metabolome,Metabolomics,Mice,Nitrogen,Phenotype,Phylogeny},
