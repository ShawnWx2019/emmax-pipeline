}

@article{maHeterosisrelatedGenesDifferent2018,
  title = {Heterosis-Related Genes under Different Planting Densities in Maize},
  author = {Ma, Juan and Zhang, Dengfeng and Cao, Yanyong and Wang, Lifeng and Li, Jingjing and L{\"u}bberstedt, Thomas and Wang, Tianyu and Li, Yu and Li, Huiyong},
  year = {2018},
