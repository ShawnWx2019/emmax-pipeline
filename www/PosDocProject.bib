@article{kubigsteltigArabidopsisMutantsAffected2003,
  title = {Arabidopsis Mutants Affected in the Transcriptional Control of Allene Oxide Synthase, the Enzyme Catalyzing the Entrance Step in Octadecanoid Biosynthesis},
  author = {Kubigsteltig, Ines I. and Weiler, Elmar W.},
