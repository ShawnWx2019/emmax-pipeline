@article{gonzalez-bayonSenescenceDefensePathways2019,
  title = {Senescence and {{Defense Pathways Contribute}} to {{Heterosis}}},
  author = {{Gonzalez-Bayon}, Rebeca and Shen, Yifei and Groszmann, Michael and Zhu, Anyu and Wang, Aihua and Allu, Annapurna D. and Dennis, Elizabeth S. and Peacock, W. James and Greaves, Ian K.},
