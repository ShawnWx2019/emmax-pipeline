@article{liAuxinSignalingGene2019,
  title = {An Auxin Signaling Gene {{BnaA3}}.{{IAA7}} Contributes to Improved Plant Architecture and Yield Heterosis in Rapeseed},
  author = {Li, Haitao and Li, Juanjuan and Song, Jurong and Zhao, Bo and Guo, Chaocheng and Wang, Bo and Zhang, Qinghua and Wang, Jing and King, Graham J. and Liu, Kede},
