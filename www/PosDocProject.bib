  title = {Genome-Wide {{Dissection}} of {{Co-selected UV-B Responsive Pathways}} in the {{UV-B Adaptation}} of {{Qingke}}},
  author = {Zeng, Xingquan and Yuan, Hongjun and Dong, Xuekui and Peng, Meng and Jing, Xinyu and Xu, Qijun and Tang, Tang and Wang, Yulin and Zha, Sang and Gao, Meng and Li, Congzhi and Shu, Chujin and Wei, Zexiu and Qimei, Wangmu and Basang, Yuzhen and Dunzhu, Jiabu and Li, Zeqing and Bai, Lijun and Shi, Jian and Zheng, Zhigang and Yu, Sibin and Fernie, Alisdair R. and Luo, Jie and Nyima, Tashi},
