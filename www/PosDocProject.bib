@article{peerVariationExpressionProtein2004,
  title = {Variation in Expression and Protein Localization of the {{PIN}} Family of Auxin Efflux Facilitator Proteins in Flavonoid Mutants with Altered Auxin Transport in {{Arabidopsis}} Thaliana},
  author = {Peer, Wendy Ann and Bandyopadhyay, Anindita and Blakeslee, Joshua J. and Makam, Srinivas N. and Chen, Rujin J. and Masson, Patrick H. and Murphy, Angus S.},
