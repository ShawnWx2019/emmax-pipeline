@article{songConservedNoncodingSequences2021,
  title = {Conserved Noncoding Sequences Provide Insights into Regulatory Sequence and Loss of Gene Expression in Maize},
  author = {Song, Baoxing and Buckler, Edward S. and Wang, Hai and Wu, Yaoyao and Rees, Evan and Kellogg, Elizabeth A. and Gates, Daniel J. and {Khaipho-Burch}, Merritt and Bradbury, Peter J. and {Ross-Ibarra}, Jeffrey and Hufford, Matthew B. and Romay, M. Cinta},
  year = {2021},
  month = may,
  journal = {Genome Research},
  issn = {1549-5469},
  doi = {10.1101/gr.266528.120},
  abstract = {Thousands of species will be sequenced in the next few years; however, understanding how their genomes work, without an unlimited budget, requires both molecular and novel evolutionary approaches. We developed a sensitive sequence alignment pipeline to identify conserved noncoding sequences (CNSs) in the Andropogoneae tribe (multiple crop species descended from a common ancestor {$\sim$}18 million years ago). The Andropogoneae share similar physiology while being tremendously genomically diverse, harboring a broad range of ploidy levels, structural variation, and transposons. These contribute to the potential of Andropogoneae as a powerful system for studying CNSs and are factors we leverage to understand the function of maize CNSs. We found that 86\% of CNSs were comprised of annotated features, including introns, UTRs, putative cis-regulatory elements, chromatin loop anchors, noncoding RNA (ncRNA) genes, and several transposable element superfamilies. CNSs were enriched in active regions of DNA replication in the early S phase of the mitotic cell cycle and showed different DNA methylation ratios compared to the genome-wide background. More than half of putative cis-regulatory sequences (identified via other methods) overlapped with CNSs detected in this study. Variants in CNSs were associated with gene expression levels, and CNS absence contributed to loss of gene expression. Furthermore, the evolution of CNSs was associated with the functional diversification of duplicated genes in the context of maize subgenomes. Our results provide a quantitative understanding of the molecular processes governing the evolution of CNSs in maize.},
  langid = {english},
  lccn = {9.043},
  pmid = {34045362},
  keywords = {/unread},
  annotation = {SC: 0000000},
  file = {/Users/shawn/Zotero/storage/HGV4PG5S/Song 等。 - 2021 - Conserved noncoding sequences provide insights int.pdf}
}

