  title = {Analysis of Transcriptional and Epigenetic Changes in Hybrid Vigor of Allopolyploid {{Brassica}} Napus Uncovers Key Roles for Small {{RNAs}}},
  author = {Shen, Yifei and Sun, Shuo and Hua, Shuijin and Shen, Enhui and Ye, Chu-Yu and Cai, Daguang and Timko, Michael P. and Zhu, Qian-Hao and Fan, Longjiang},
  year = {2017},
  journal = {The Plant Journal},
