@article{mibeiMetabolomicAnalysesEvaluate2018,
  title = {Metabolomic Analyses to Evaluate the Effect of Drought Stress on Selected {{African Eggplant}} Accessions},
  author = {Mibei, Elias K and Owino, Willis O and Ambuko, Jane and Giovannoni, James J and Onyango, Arnold N},
