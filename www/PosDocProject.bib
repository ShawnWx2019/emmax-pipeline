@article{owensBiochemicalGeneticCharacterization2008,
  title = {Biochemical and Genetic Characterization of {{Arabidopsis}} Flavanone 3{$\beta$}-Hydroxylase},
  author = {Owens, Daniel K. and Crosby, Kevin C. and Runac, Justin and Howard, Brad A. and Winkel, Brenda S. J.},
