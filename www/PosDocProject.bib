  author = {Guo, Kai and Gao, Wei and Zhang, Tao-rui and Wang, Zu-ying and Sun, Xiao-ting and Yang, Peng and Long, Lu and Liu, Xue-ying and Wang, Wen-wen and Teng, Zhong-hua and Liu, Da-jun and Liu, De-xin and Tu, Li-li and Zhang, Zheng-sheng},
