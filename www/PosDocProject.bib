}

@article{liTranscriptomeDNAMethylome2018,
  title = {Transcriptome and {{DNA}} Methylome Reveal Insights into Yield Heterosis in the Curds of Broccoli ({{Brassica}} Oleracea {{L}} Var. Italic)},
  author = {Li, Hui and Yuan, Jiye and Wu, Mei and Han, Zhanpin and Li, Lihong and Jiang, Hanmin and Jia, Yinglan and Han, Xue and Liu, Min and Sun, Deling and Chen, Chengbin and Song, Wenqin and Wang, Chunguo},
