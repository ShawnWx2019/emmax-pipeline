@article{hazratiIntegratedLCMSGCMSBased2022,
  title = {Integrated {{LC-MS}} and {{GC-MS-Based Metabolomics Reveal}} the {{Effects}} of {{Plant Competition}} on the {{Rye Metabolome}}},
  author = {Hazrati, Hossein and Kudsk, Per and Ding, Ling and Uthe, Henriette and Fomsgaard, Inge S.},
