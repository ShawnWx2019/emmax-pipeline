@article{jiaAnchoreneCarotenoidderivedRegulatory2019,
  title = {Anchorene Is a Carotenoid-Derived Regulatory Metabolite Required for Anchor Root Formation in {{Arabidopsis}}},
  author = {Jia, Kun-Peng and Dickinson, Alexandra J. and Mi, Jianing and Cui, Guoxin and Xiao, Ting Ting and Kharbatia, Najeh M. and Guo, Xiujie and Sugiono, Erli and Aranda, Manuel and Blilou, Ikram and Rueping, Magnus and Benfey, Philip N. and {Al-Babili}, Salim},
