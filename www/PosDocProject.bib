  lccn = {4.116},
  keywords = {/unread,F-box protein (FBP),miRNA,Phytohormone signalling,Plant development,Plant stresses,SCF complex,Secondary metabolites,Ubiquitin 26S-proteasome system (UPS)}
}

@article{abidIncreasingFloralVisitation2022,
  title = {Increasing Floral Visitation and Hybrid Seed Production Mediated by Beauty Mark in {{Gossypium}} Hirsutum},
