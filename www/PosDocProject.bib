@article{fuPartialLeastSquares2012,
  title = {Partial Least Squares Regression, Support Vector Machine Regression, and Transcriptome-Based Distances for Prediction of Maize Hybrid Performance with Gene Expression Data},
  author = {Fu, Junjie and Falke, K. Christin and Thiemann, Alexander and Schrag, Tobias A. and Melchinger, Albrecht E. and Scholten, Stefan and Frisch, Matthias},
  year = {2012},
