  keywords = {/unread,agronomic trait,Edible Grain,{Genes, Plant},Genetic Association Studies,mature seed,metabolic prediction,metabolic quantitative trait loci,Metabolomics,Plant Breeding,{Polymorphism, Single Nucleotide},Quantitative Trait Loci,{Quantitative Trait, Heritable},Triticum,Triticum aestivum L.},
