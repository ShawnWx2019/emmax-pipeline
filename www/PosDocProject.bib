@article{kawashimaInterplaySLIM1MiR3952011,
  title = {Interplay of {{SLIM1}} and {{miR395}} in the Regulation of Sulfate Assimilation in {{Arabidopsis}}},
  author = {Kawashima, Cintia G. and Matthewman, Colette A. and Huang, Siqi and Lee, Bok-Rye and Yoshimoto, Naoko and Koprivova, Anna and Rubio-Somoza, Ignacio and Todesco, Marco and Rathjen, Tina and Saito, Kazuki and Takahashi, Hideki and Dalmay, Tamas and Kopriva, Stanislav},
  year = {2011},
