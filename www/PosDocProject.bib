@article{flagelEvolutionaryRateVariation2010,
  title = {Evolutionary Rate Variation, Genomic Dominance and Duplicate Gene Expression Evolution during Allotetraploid Cotton Speciation},
  author = {Flagel, Lex E. and Wendel, Jonathan F.},
