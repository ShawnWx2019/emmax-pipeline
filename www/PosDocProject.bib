  title = {Flavanols: {{Catechins}} and {{Proanthocyanidins}}},
  shorttitle = {Flavanols},
  booktitle = {Natural {{Products}}: {{Phytochemistry}}, {{Botany}} and {{Metabolism}} of {{Alkaloids}}, {{Phenolics}} and {{Terpenes}}},
