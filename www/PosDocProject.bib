  title = {Flavonoid Biosynthesis Controls Fiber Color in Naturally Colored Cotton},
  author = {Liu, Hai-Feng and Luo, Cheng and Song, Wu and Shen, Haitao and Li, Guoliang and He, Zhi-Gang and Chen, Wen-Gang and Cao, Yan-Yan and Huang, Fang and Tang, Shou-Wu and Hong, Ping and Zhao, En-Feng and Zhu, Jianbo and He, Dajun and Wang, Shaoming and Huo, Guang-Ying and Liu, Hailiang},
  year = {2018},
