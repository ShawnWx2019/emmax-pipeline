  lccn = {5.753},
  keywords = {/unread,log2fc,植物病原},
  annotation = {SC: None[s0]},
  file = {/Users/shawn/Zotero/storage/4ZLY8G57/Wang 等。 - 2017 - Identification of Circular RNAs in Kiwifruit and T.pdf}
}

@article{wangInsightCatechinsMetabolic2018,
