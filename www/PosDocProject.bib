@article{roepkeApoplasticVGlucosidaseEssential2017,
  title = {An {{Apoplastic}} {$\beta$}-{{Glucosidase}} Is {{Essential}} for the {{Degradation}} of {{Flavonol}} 3-{{O-$\beta$-Glucoside-7-O-$\alpha$-Rhamnosides}} in {{Arabidopsis}}},
  author = {Roepke, Jonathon and Gordon, Harley O.W. and Neil, Kevin J.A. and Gidda, Satinder and Mullen, Robert T. and Freixas Coutin, Jos{\'e} A. and {Bray-Stone}, Delaney and Bozzo, Gale G.},
  year = {2017},
