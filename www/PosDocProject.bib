@article{burtNaturalVariationHydroxycinnamic2019,
  title = {Natural Variation of Hydroxycinnamic Acid Amides in Maize Landraces},
  author = {Burt, Andrew J. and Arnason, John T. and {Garc{\'i}a-Lara}, Silverio},
  year = {2019},
