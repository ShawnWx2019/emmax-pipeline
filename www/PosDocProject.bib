@article{caoMiR319TargetedGhTCP4Promotes2020,
  title = {The {{miR319-Targeted GhTCP4 Promotes}} the {{Transition}} from {{Cell Elongation}} to {{Wall Thickening}} in {{Cotton Fiber}}},
  author = {Cao, Jun-Feng and Zhao, Bo and Huang, Chao-Chen and Chen, Zhi-Wen and Zhao, Ting and Liu, Hong-Ru and Hu, Guan-Jing and Shangguan, Xiao-Xia and Shan, Chun-Min and Wang, Ling-Jian and Zhang, Tian-Zhen and Wendel, Jonathan F. and Guan, Xue-Ying and Chen, Xiao-Ya},
