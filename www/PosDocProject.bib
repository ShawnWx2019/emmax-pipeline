@article{sunChromosomelevelGenomeIsodon2022,
  title = {A Chromosome-Level Genome of {{Isodon}} Rubescens Reveals That Tandem-Duplicated {{CYP706V}} Oxidase Genes Control Oridonin Biosynthesis in Shoot Apex},
  author = {Sun, Yuwei and Shao, Jie and Liu, Haili and Wang, Hua and Wang, Guangyi and Li, Jianhua and Mao, Yaping and Chen, Zhuo and Ma, Ke and Xu, Lin and Wang, Yong},
  year = {2022},
  month = dec,
  journal = {Molecular Plant},
  issn = {1674-2052},
  doi = {10.1016/j.molp.2022.12.007},
  urldate = {2022-12-17},
  abstract = {The ent-kaurenoids (e.g., oridonin, enmein) from Isodon genus (Lamiaceae) are one class of diterpenoids with rich structural diversity and intriguing pharmaceutical activities. In contrast to the well-established gibberellins pathway, oxidative modifications diversifying ent-kaurene skeleton in Isodon remain undetermined for a half century. Here, we report a chromosome-level genome of I. rubescens, a well-recognized oridonin producer long been favored by Asian people as a traditional herb with antitumour effects. The shoot apex was proposed to be the actual region actively producing ent-kaurene diterpenoids. Through comparative genomics and phylogenetic analyses, we discovered a cluster of tandem-duplicated CYP706V oxygenase-encoding genes located on an ancient genomic block widely distributed in eudicots, whereas almost exclusively emerged in Isodon plants. In shoot apex, IrCYP706V2 and IrCYP706V7 oxidized ent-kaurene core in the initial stage of oridonin biosynthesis. Loss of CYP706Vs in the other Lamiaceae plants offered an explanation for the specific kaurenoid production in Isodon plants. Moreover, the presence of multiple diterpenoid synthases in the Isodon genome also revealed a great potential of generating diterpenoid diversity. These findings provided insights into the evolution of lineage-specific diterpenoid pathway, and laid a foundation of improving the production of bioactive ent-kaurene-type diterpenoids by molecular breeding and synthetic biology approaches.},
  langid = {english},
  keywords = {CYP706 oxidases,diterpenoid diversity,genome sequencing,oridonin biosynthesis},
  file = {/Users/shawn/Zotero/storage/Q7I5E6ZZ/Sun et al. - 2022 - A chromosome-level genome of Isodon rubescens reve.pdf;/Users/shawn/Zotero/storage/CTVYXZWH/S1674205222004427.html}
}

@article{sunExtensiveIntraspecificGene2018,
