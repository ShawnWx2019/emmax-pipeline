@article{kleinboeltingStructuralFeaturesThousands2015,
  title = {The {{Structural Features}} of {{Thousands}} of {{T-DNA Insertion Sites Are Consistent}} with a {{Double-Strand Break Repair-Based Insertion Mechanism}}},
  author = {Kleinboelting, Nils and Huep, Gunnar and Appelhagen, Ingo and Viehoever, Prisca and Li, Yong and Weisshaar, Bernd},
