@article{fanSystematicErrorRemoval2019,
  title = {Systematic {{Error Removal Using Random Forest}} for {{Normalizing Large-Scale Untargeted Lipidomics Data}}},
  author = {Fan, Sili and Kind, Tobias and Cajka, Tomas and Hazen, Stanley L. and Tang, W. H. Wilson and {Kaddurah-Daouk}, Rima and Irvin, Marguerite R. and Arnett, Donna K. and Barupal, Dinesh K. and Fiehn, Oliver},
