@article{shiMetabolomicsAnalysisMetaboliteagronomic2020,
  title = {Metabolomics Analysis and Metabolite-Agronomic Trait Associations Using Kernels of Wheat ({{Triticum}} Aestivum) Recombinant Inbred Lines},
  author = {Shi, Taotao and Zhu, Anting and Jia, Jingqi and Hu, Xin and Chen, Jie and Liu, Wei and Ren, Xifeng and Sun, Dongfa and Fernie, Alisdair R. and Cui, Fa and Chen, Wei},
  year = {2020},
  month = jul,
  journal = {The Plant Journal: For Cell and Molecular Biology},
  volume = {103},
