@article{chananaHcapcaAutomatedHierarchical2020,
  title = {Hcapca: {{Automated Hierarchical Clustering}} and {{Principal Component Analysis}} of {{Large Metabolomic Datasets}} in {{R}}},
  shorttitle = {Hcapca},
  author = {Chanana, Shaurya and Thomas, Chris S. and Zhang, Fan and Rajski, Scott R. and Bugni, Tim S.},
