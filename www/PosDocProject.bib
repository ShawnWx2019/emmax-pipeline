@article{fengTranscriptomeAnalysisDifferent2022,
  title = {Transcriptome {{Analysis}} of {{Different Sections}} of {{Rhizome}} in {{Polygonatum}} Sibiricum {{Red}}. and {{Mining Putative Genes Participate}} in {{Polysaccharide Biosynthesis}}},
