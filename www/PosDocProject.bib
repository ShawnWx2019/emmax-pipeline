  author = {Wang, Guo-Ming and Yin, Hao and Qiao, Xin and Tan, Xu and Gu, Chao and Wang, Bao-Hua and Cheng, Rui and Wang, Ying-Zhen and Zhang, Shao-Ling},
  year = {2016},
