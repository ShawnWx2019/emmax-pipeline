@article{liangMetabolomicsdrivenGeneMining2021,
  title = {Metabolomics-Driven Gene Mining and Genetic Improvement of Tolerance to Salt-Induced Osmotic Stress in Maize},
  author = {Liang, Xiaoyan and Liu, Songyu and Wang, Tao and Li, Fenrong and Cheng, Jinkui and Lai, Jinsheng and Qin, Feng and Li, Zhen and Wang, Xiangfeng and Jiang, Caifu},
  year = {2021},
