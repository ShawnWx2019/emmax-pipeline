@article{litholdoCircularRNAsPlant2018,
  title = {Circular {{RNAs}} and {{Plant Stress Responses}}},
  author = {Litholdo, Celso Gaspar and {da Fonseca}, Guilherme Cordenonsi},
  year = {2018},
