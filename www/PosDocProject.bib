@article{liuCalciumTransporterANNEXIN12020,
  title = {The Calcium Transporter {{ANNEXIN1}} Mediates Cold-Induced Calcium Signaling and Freezing Tolerance in Plants},
  author = {Liu, Qiangbo and Ding, Yanglin and Shi, Yiting and Ma, Liang and Wang, Yi and Song, Chunpeng and Wilkins, Katie A and Davies, Julia M and Knight, Heather and Knight, Marc R and Gong, Zhizhong and Guo, Yan and Yang, Shuhua},
